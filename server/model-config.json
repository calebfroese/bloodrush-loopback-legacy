{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongoDB"
  },
  "AccessToken": {
    "dataSource": "mongoDB",
    "public": false
  },
  "ACL": {
    "dataSource": "mongoDB",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongoDB",
    "public": false
  },
  "Role": {
    "dataSource": "mongoDB",
    "public": false
  },
  "league": {
    "dataSource": "mongoDB",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "generateSeason": true
        }
      }
    }
  },
  "season": {
    "dataSource": "mongoDB",
    "public": true
  },
  "team": {
    "dataSource": "mongoDB",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "generate": true,
          "score": true
        }
      }
    }
  },
  "player": {
    "dataSource": "mongoDB",
    "public": true
  },
  "email": {
    "dataSource": "mongoDB",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "sendActivation": true,
          "verifyEmail": true,
          "addToMailingList": true
        }
      }
    }
  },
  "image": {
    "dataSource": "mongoDB",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "createPreview": true,
          "createPlayers": true
        }
      }
    }
  },
  "game": {
    "dataSource": "mongoDB",
    "public": true
  }
}
